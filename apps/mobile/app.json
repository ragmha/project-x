{
  "expo": {
    "name": "project-x",
    "slug": "project-x",
    "scheme": "project-x",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.raghib.projectx",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "io.raghib.projectx",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "react-native-health"],
    "extra": {
      "eas": {
        "projectId": "cbff6048-ba4f-445b-bb69-d387e92ef460"
      }
    }
  }
}
